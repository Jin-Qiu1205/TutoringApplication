@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Admin Dashboard</h2>
<p>Manage users in the system:</p>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {

            <tr><td>@user.Email</td></tr>
        }
    </tbody>
</table>

